{
  "manifest_version": 3,
  "name": "AI Translator",
  "version": "0.3.0",
  "description": "AI Translator",
  "icons": {
    "16": "assets/icons/icon_16.png",
    "32": "assets/icons/icon_32.png",
    "48": "assets/icons/icon_48.png",
    "128": "assets/icons/icon_128.png"
  },
  "action": {
    "default_icon": "assets/icons/icon.png",
    "default_title": "AI Translator"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "./dist/content.css"
      ],
      "js": [
        "./dist/content.js"
      ]
    }
  ]
}